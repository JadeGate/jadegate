{
  "jade_version": "1.0.0",
  "skill_id": "mcp_youtube_search",
  "metadata": {
    "name": "MCP YouTube Search",
    "version": "1.0.0",
    "description": "Search YouTube videos.",
    "author": "jade-mcp-bridge",
    "tags": [
      "mcp",
      "youtube",
      "search",
      "video"
    ],
    "license": "BSL-1.1",
    "created_at": "2026-02-21T00:00:00Z",
    "updated_at": "2026-02-21T00:00:00Z"
  },
  "trigger": {
    "type": "task_intent",
    "conditions": [
      {
        "field": "task.type",
        "operator": "in",
        "value": [
          "mcp_youtube_search"
        ]
      }
    ]
  },
  "input_schema": {
    "required_params": [
      {
        "name": "query",
        "type": "string",
        "description": "Search query"
      }
    ],
    "optional_params": [
      {
        "name": "max_results",
        "type": "number",
        "description": "Max results",
        "default": 5
      }
    ]
  },
  "output_schema": {
    "fields": [
      {
        "name": "videos",
        "type": "array",
        "description": "Video results"
      }
    ]
  },
  "execution_dag": {
    "nodes": [
      {
        "id": "search",
        "action": "http_get",
        "params": {
          "url": "https://www.googleapis.com/youtube/v3/search?part=snippet&q={{input.query}}&maxResults={{input.max_results | default:5}}&type=video&key={{env.YOUTUBE_API_KEY}}",
          "timeout_ms": 10000
        }
      },
      {
        "id": "check",
        "action": "condition_check",
        "params": {
          "field": "{{search.output.status_code}}",
          "operator": "equals",
          "value": 200
        }
      },
      {
        "id": "extract",
        "action": "json_extract",
        "params": {
          "data": "{{search.output.body}}",
          "path": "$.items[*]",
          "fields": [
            "id",
            "snippet"
          ]
        }
      },
      {
        "id": "ret",
        "action": "return_result",
        "params": {
          "videos": "{{extract.output.value}}"
        }
      }
    ],
    "edges": [
      {
        "from": "search",
        "to": "check"
      },
      {
        "from": "check",
        "to": "extract"
      },
      {
        "from": "extract",
        "to": "ret"
      }
    ],
    "entry_node": "search",
    "exit_node": [
      "ret"
    ]
  },
  "security": {
    "network_whitelist": [
      "www.googleapis.com"
    ],
    "file_permissions": {
      "read": [],
      "write": []
    },
    "max_execution_time_ms": 15000,
    "max_retries": 1,
    "sandbox_level": "standard",
    "dangerous_patterns": [],
    "env_whitelist": [
      "YOUTUBE_API_KEY"
    ]
  },
  "mcp_compatible": true,
  "required_mcp_capabilities": [
    "youtube"
  ],
  "jade_signature": {
    "version": "1.0.0",
    "skill_id": "mcp_youtube_search",
    "content_hash": "sha256:c3d586dcb7185f7aee22f4c0a8d6d4e8ff22968f43b0a1a9ae97623bb8cbaed6",
