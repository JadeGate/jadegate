{
  "jade_version": "1.0.0",
  "skill_id": "mcp_redis_set",
  "metadata": {
    "name": "MCP Redis Set",
    "version": "1.0.0",
    "description": "Set key-value pair in Redis.",
    "author": "jade-mcp-bridge",
    "tags": [
      "mcp",
      "redis",
      "set",
      "cache"
    ],
    "license": "BSL-1.1",
    "created_at": "2026-02-21T00:00:00Z",
    "updated_at": "2026-02-21T00:00:00Z"
  },
  "trigger": {
    "type": "task_intent",
    "conditions": [
      {
        "field": "task.type",
        "operator": "in",
        "value": [
          "mcp_redis_set"
        ]
      }
    ]
  },
  "input_schema": {
    "required_params": [
      {
        "name": "key",
        "type": "string",
        "description": "Redis key"
      },
      {
        "name": "value",
        "type": "string",
        "description": "Value to set"
      }
    ],
    "optional_params": []
  },
  "output_schema": {
    "fields": [
      {
        "name": "success",
        "type": "boolean",
        "description": "Success"
      }
    ]
  },
  "execution_dag": {
    "nodes": [
      {
        "id": "set",
        "action": "redis_set",
        "params": {
          "key": "{{input.key}}",
          "value": "{{input.value}}"
        }
      },
      {
        "id": "ret",
        "action": "return_result",
        "params": {
          "success": "{{set.output.success}}"
        }
      }
    ],
    "edges": [
      {
        "from": "set",
        "to": "ret"
      }
    ],
    "entry_node": "set",
    "exit_node": [
      "ret"
    ]
  },
  "security": {
    "network_whitelist": [],
    "file_permissions": {
      "read": [],
      "write": []
    },
    "max_execution_time_ms": 15000,
    "max_retries": 1,
    "sandbox_level": "strict",
    "dangerous_patterns": []
  },
  "mcp_compatible": true,
  "required_mcp_capabilities": [
    "redis"
  ]
}