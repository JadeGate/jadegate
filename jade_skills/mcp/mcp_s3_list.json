{
  "jade_version": "1.0.0",
  "skill_id": "mcp_s3_list",
  "metadata": {
    "name": "MCP AWS S3 List Objects",
    "version": "1.0.0",
    "description": "List objects in S3 bucket.",
    "author": "jade-mcp-bridge",
    "tags": [
      "mcp",
      "aws",
      "s3",
      "storage"
    ],
    "license": "BSL-1.1",
    "created_at": "2026-02-21T00:00:00Z",
    "updated_at": "2026-02-21T00:00:00Z"
  },
  "trigger": {
    "type": "task_intent",
    "conditions": [
      {
        "field": "task.type",
        "operator": "in",
        "value": [
          "mcp_s3_list"
        ]
      }
    ]
  },
  "input_schema": {
    "required_params": [
      {
        "name": "bucket",
        "type": "string",
        "description": "S3 bucket name"
      }
    ],
    "optional_params": [
      {
        "name": "prefix",
        "type": "string",
        "description": "Key prefix filter",
        "default": ""
      },
      {
        "name": "region",
        "type": "string",
        "description": "AWS region",
        "default": "us-east-1"
      }
    ]
  },
  "output_schema": {
    "fields": [
      {
        "name": "objects",
        "type": "array",
        "description": "S3 objects"
      }
    ]
  },
  "execution_dag": {
    "nodes": [
      {
        "id": "list",
        "action": "http_get",
        "params": {
          "url": "https://s3.amazonaws.com/{{input.bucket}}",
          "headers": {
            "Authorization": "{{env.AWS_AUTH}}"
          },
          "timeout_ms": 15000
        }
      },
      {
        "id": "check",
        "action": "condition_check",
        "params": {
          "field": "{{list.output.status_code}}",
          "operator": "equals",
          "value": 200
        }
      },
      {
        "id": "parse",
        "action": "xml_parse",
        "params": {
          "xml": "{{list.output.body}}"
        }
      },
      {
        "id": "ret",
        "action": "return_result",
        "params": {
          "objects": "{{parse.output.data}}"
        }
      }
    ],
    "edges": [
      {
        "from": "list",
        "to": "check"
      },
      {
        "from": "check",
        "to": "parse"
      },
      {
        "from": "parse",
        "to": "ret"
      }
    ],
    "entry_node": "list",
    "exit_node": [
      "ret"
    ]
  },
  "security": {
    "network_whitelist": [
      "*.amazonaws.com",
      "s3.amazonaws.com"
    ],
    "file_permissions": {
      "read": [],
      "write": []
    },
    "max_execution_time_ms": 15000,
    "max_retries": 1,
    "sandbox_level": "standard",
    "dangerous_patterns": []
  },
  "mcp_compatible": true,
  "required_mcp_capabilities": [
    "aws-s3"
  ]
}