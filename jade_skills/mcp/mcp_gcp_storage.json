{
  "jade_version": "1.0.0",
  "skill_id": "mcp_gcp_storage",
  "metadata": {
    "name": "MCP GCP Cloud Storage",
    "version": "1.0.0",
    "description": "List objects in a Google Cloud Storage bucket.",
    "author": "jade-mcp-bridge",
    "tags": [
      "mcp",
      "gcp",
      "storage",
      "cloud"
    ],
    "license": "BSL-1.1",
    "created_at": "2026-02-22T17:49:27Z",
    "updated_at": "2026-02-22T17:49:27Z"
  },
  "trigger": {
    "type": "task_intent",
    "conditions": [
      {
        "field": "task.type",
        "operator": "in",
        "value": [
          "mcp_gcp_storage"
        ]
      }
    ]
  },
  "input_schema": {
    "required_params": [
      {
        "name": "bucket",
        "type": "string",
        "description": "GCS bucket name"
      }
    ],
    "optional_params": [
      {
        "name": "prefix",
        "type": "string",
        "description": "Object prefix filter",
        "default": ""
      }
    ]
  },
  "output_schema": {
    "fields": [
      {
        "name": "results",
        "type": "array",
        "description": "Storage objects"
      },
      {
        "name": "status",
        "type": "string",
        "description": "Status"
      }
    ]
  },
  "execution_dag": {
    "nodes": [
      {
        "id": "fetch",
        "action": "http_get",
        "params": {
          "url": "https://storage.googleapis.com/storage/v1/b/{{input.bucket}}/o?prefix={{input.prefix}}",
          "headers": {
            "Authorization": "Bearer {{env.GCP_TOKEN}}"
          }
        }
      },
      {
        "id": "chk",
        "action": "condition_check",
        "params": {
          "field": "{{fetch.output.status_code}}",
          "operator": "equals",
          "value": 200
        }
      },
      {
        "id": "ext",
        "action": "json_extract",
        "params": {
          "data": "{{fetch.output.body}}",
          "path": "$.items[*]",
          "fields": [
            "name",
            "size",
            "updated",
            "contentType"
          ]
        }
      },
      {
        "id": "ret",
        "action": "return_result",
        "params": {
          "results": "{{ext.output.value}}",
          "status": "ok"
        }
      }
    ],
    "edges": [
      {
        "from": "fetch",
        "to": "chk"
      },
      {
        "from": "chk",
        "to": "ext"
      },
      {
        "from": "ext",
        "to": "ret"
      }
    ],
    "entry_node": "fetch",
    "exit_node": [
      "ret"
    ]
  },
  "security": {
    "network_whitelist": [
      "storage.googleapis.com"
    ],
    "file_permissions": {
      "read": [],
      "write": []
    },
    "max_execution_time_ms": 15000,
    "max_retries": 1,
    "sandbox_level": "standard",
    "dangerous_patterns": [],
    "env_whitelist": [
      "GCP_TOKEN"
    ]
  },
  "mcp_compatible": true,
  "required_mcp_capabilities": [
    "gcp-storage"
  ],
  "jade_signature": {
    "version": "1.0.0",
