{
  "jade_version": "1.0.0",
  "skill_id": "mcp_stackoverflow_search",
  "metadata": {
    "name": "MCP StackOverflow Search",
    "version": "1.0.0",
    "description": "Search questions on StackOverflow.",
    "author": "jade-mcp-bridge",
    "tags": [
      "mcp",
      "stackoverflow",
      "search",
      "qa"
    ],
    "license": "BSL-1.1",
    "created_at": "2026-02-22T17:49:27Z",
    "updated_at": "2026-02-22T17:49:27Z"
  },
  "trigger": {
    "type": "task_intent",
    "conditions": [
      {
        "field": "task.type",
        "operator": "in",
        "value": [
          "mcp_stackoverflow_search"
        ]
      }
    ]
  },
  "input_schema": {
    "required_params": [
      {
        "name": "query",
        "type": "string",
        "description": "Search query"
      }
    ],
    "optional_params": [
      {
        "name": "limit",
        "type": "number",
        "description": "Max results",
        "default": 10
      }
    ]
  },
  "output_schema": {
    "fields": [
      {
        "name": "results",
        "type": "array",
        "description": "Questions"
      },
      {
        "name": "status",
        "type": "string",
        "description": "Status"
      }
    ]
  },
  "execution_dag": {
    "nodes": [
      {
        "id": "search",
        "action": "http_get",
        "params": {
          "url": "https://api.stackexchange.com/2.3/search/advanced?order=desc&sort=relevance&q={{input.query}}&site=stackoverflow&pagesize={{input.limit}}"
        }
      },
      {
        "id": "chk",
        "action": "condition_check",
        "params": {
          "field": "{{search.output.status_code}}",
          "operator": "equals",
          "value": 200
        }
      },
      {
        "id": "ext",
        "action": "json_extract",
        "params": {
          "data": "{{search.output.body}}",
          "path": "$.items[*]",
          "fields": [
            "title",
            "link",
            "score",
            "answer_count",
            "is_answered",
            "tags"
          ]
        }
      },
      {
        "id": "ret",
        "action": "return_result",
        "params": {
          "results": "{{ext.output.value}}",
          "status": "ok"
        }
      }
    ],
    "edges": [
      {
        "from": "search",
        "to": "chk"
      },
      {
        "from": "chk",
        "to": "ext"
      },
      {
        "from": "ext",
        "to": "ret"
      }
    ],
    "entry_node": "search",
    "exit_node": [
      "ret"
    ]
  },
  "security": {
    "network_whitelist": [
      "api.stackexchange.com"
    ],
    "file_permissions": {
      "read": [],
      "write": []
    },
    "max_execution_time_ms": 15000,
    "max_retries": 1,
    "sandbox_level": "standard",
    "dangerous_patterns": []
  },
  "mcp_compatible": true,
  "required_mcp_capabilities": [
    "stackoverflow"
  ],
  "jade_signature": {
    "version": "1.0.0",
    "skill_id": "mcp_stackoverflow_search",
