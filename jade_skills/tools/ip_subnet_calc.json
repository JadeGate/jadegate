{
  "jade_version": "1.0.0",
  "skill_id": "ip_subnet_calc",
  "metadata": {
    "name": "IP Subnet Calculator",
    "version": "1.0.0",
    "description": "Calculate subnet details from CIDR notation.",
    "author": "jadegate",
    "tags": [
      "ip",
      "subnet",
      "network",
      "cidr"
    ],
    "license": "BSL-1.1",
    "created_at": "2026-02-22T17:50:21Z",
    "updated_at": "2026-02-22T17:50:21Z"
  },
  "trigger": {
    "type": "task_intent",
    "conditions": [
      {
        "field": "task.type",
        "operator": "in",
        "value": [
          "ip_subnet_calc"
        ]
      }
    ]
  },
  "input_schema": {
    "required_params": [
      {
        "name": "cidr",
        "type": "string",
        "description": "CIDR notation (e.g. 192.168.1.0/24)"
      }
    ],
    "optional_params": []
  },
  "output_schema": {
    "fields": [
      {
        "name": "subnet_info",
        "type": "object",
        "description": "Subnet details"
      },
      {
        "name": "status",
        "type": "string",
        "description": "Status"
      }
    ]
  },
  "execution_dag": {
    "nodes": [
      {
        "id": "compute",
        "action": "compute",
        "params": {
          "expression": "subnet",
          "input": "{{input.cidr}}"
        }
      },
      {
        "id": "ret",
        "action": "return_result",
        "params": {
          "subnet_info": "{{compute.output.value}}",
          "status": "ok"
        }
      }
    ],
    "edges": [
      {
        "from": "compute",
        "to": "ret"
      }
    ],
    "entry_node": "compute",
    "exit_node": [
      "ret"
    ]
  },
  "security": {
    "network_whitelist": [],
    "file_permissions": {
      "read": [],
      "write": []
    },
    "max_execution_time_ms": 10000,
    "max_retries": 1,
    "sandbox_level": "strict",
    "dangerous_patterns": []
  },
