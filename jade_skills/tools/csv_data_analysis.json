{
  "jade_version": "1.0.0",
  "skill_id": "csv_data_analysis",
  "metadata": {
    "name": "CSV Data Analysis",
    "version": "1.0.0",
    "description": "Read and analyze CSV files with column stats, filtering and sorting",
    "author": "jadegate-community",
    "tags": [
      "csv",
      "data",
      "analysis",
      "statistics"
    ],
    "license": "MIT",
    "created_at": "2026-02-21T03:00:00Z",
    "updated_at": "2026-02-21T03:00:00Z"
  },
  "trigger": {
    "type": "task_intent",
    "conditions": [
      {
        "field": "task.type",
        "operator": "in",
        "value": [
          "analyze_csv",
          "csv_analysis"
        ]
      }
    ]
  },
  "input_schema": {
    "required_params": [
      {
        "name": "file_path",
        "type": "string",
        "description": "Path to CSV file"
      },
      {
        "name": "operation",
        "type": "string",
        "description": "Operation: summary, filter, sort"
      }
    ]
  },
  "output_schema": {
    "fields": [
      {
        "name": "rows",
        "type": "number",
        "description": "Total rows"
      },
      {
        "name": "columns",
        "type": "array",
        "description": "Column names"
      },
      {
        "name": "result",
        "type": "string",
        "description": "Analysis result"
      }
    ]
  },
  "execution_dag": {
    "nodes": [
      {
        "id": "read_csv",
        "action": "file_read",
        "params": {
          "path": "{{input.file_path}}",
          "encoding": "utf-8"
        }
      },
      {
        "id": "check_read",
        "action": "condition_check",
        "params": {
          "field": "{{read_csv.output.content}}",
          "operator": "exists",
          "value": true
        }
      },
      {
        "id": "parse",
        "action": "data_transform",
        "params": {
          "input": "{{read_csv.output.content}}",
          "operation": "csv_to_json"
        }
      },
      {
        "id": "analyze",
        "action": "data_transform",
        "params": {
          "input": "{{parse.output.result}}",
          "operation": "{{input.operation}}"
        }
      },
      {
        "id": "ret_ok",
        "action": "return_result",
        "params": {
          "rows": "{{parse.output.row_count}}",
          "columns": "{{parse.output.columns}}",
          "result": "{{analyze.output.result}}"
        }
      },
      {
        "id": "ret_fail",
        "action": "return_error",
        "params": {
          "error_code": "CSV_ERROR",
          "message": "Failed to read or parse CSV"
        }
      }
    ],
    "edges": [
      {
        "from": "read_csv",
        "to": "check_read"
      },
      {
        "from": "check_read",
        "to": "parse",
        "condition": "success"
      },
      {
        "from": "check_read",
        "to": "ret_fail",
        "condition": "failure"
      },
      {
        "from": "parse",
        "to": "analyze"
      },
      {
        "from": "analyze",
        "to": "ret_ok"
      }
    ],
    "entry_node": "read_csv",
    "exit_node": [
      "ret_ok",
      "ret_fail"
    ]
  },
  "security": {
    "network_whitelist": [],
    "file_permissions": {
      "read": [
        "{{input.file_path}}"
      ],
      "write": []
    },
    "max_execution_time_ms": 30000,
    "max_retries": 2,
    "sandbox_level": "strict",
    "dangerous_patterns": []
  },
  "mcp_compatible": true,
  "required_mcp_capabilities": [
    "file"
  ],
  "jade_signature": {
    "version": "1.0.0",
    "skill_id": "csv_data_analysis",
    "content_hash": "sha256:0eeba386128a8628dde87d6371f0630575fe106df05cf4f7ecd625ad6d996771",
