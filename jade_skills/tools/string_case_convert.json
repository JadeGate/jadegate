{
  "jade_version": "1.0.0",
  "skill_id": "string_case_convert",
  "metadata": {
    "name": "String Case Converter",
    "version": "1.0.0",
    "description": "Convert string between cases (camelCase, snake_case, kebab-case, etc).",
    "author": "jadegate",
    "tags": [
      "string",
      "case",
      "convert",
      "utility"
    ],
    "license": "BSL-1.1",
    "created_at": "2026-02-22T17:50:21Z",
    "updated_at": "2026-02-22T17:50:21Z"
  },
  "trigger": {
    "type": "task_intent",
    "conditions": [
      {
        "field": "task.type",
        "operator": "in",
        "value": [
          "string_case_convert"
        ]
      }
    ]
  },
  "input_schema": {
    "required_params": [
      {
        "name": "text",
        "type": "string",
        "description": "Input string"
      },
      {
        "name": "target_case",
        "type": "string",
        "description": "Target case: camel, snake, kebab, pascal, upper, lower"
      }
    ],
    "optional_params": []
  },
  "output_schema": {
    "fields": [
      {
        "name": "converted",
        "type": "string",
        "description": "Converted string"
      },
      {
        "name": "status",
        "type": "string",
        "description": "Status"
      }
    ]
  },
  "execution_dag": {
    "nodes": [
      {
        "id": "convert",
        "action": "text_replace",
        "params": {
          "data": "{{input.text}}",
          "pattern": "case_convert",
          "replacement": "{{input.target_case}}"
        }
      },
      {
        "id": "ret",
        "action": "return_result",
        "params": {
          "converted": "{{convert.output.value}}",
          "status": "ok"
        }
      }
    ],
    "edges": [
      {
        "from": "convert",
        "to": "ret"
      }
    ],
    "entry_node": "convert",
    "exit_node": [
      "ret"
    ]
  },
  "security": {
    "network_whitelist": [],
    "file_permissions": {
      "read": [],
      "write": []
    },
    "max_execution_time_ms": 10000,
    "max_retries": 1,
    "sandbox_level": "strict",
    "dangerous_patterns": []
  },
  "jade_signature": {
