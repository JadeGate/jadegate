{
  "jade_version": "1.0.0",
  "metadata": {
    "version": "1.0.0",
    "author": "jadegate-official",
    "name": "UUID Generator",
    "description": "Generate UUID v4 identifiers",
    "tags": [
      "uuid",
      "generator",
      "utility"
    ]
  },
  "trigger": {
    "type": "task_intent",
    "conditions": [
      {
        "field": "task.type",
        "operator": "in",
        "value": [
          "uuid_generator"
        ]
      }
    ]
  },
  "security": {
    "sandbox_level": "standard",
    "max_execution_time_ms": 15000,
    "max_retries": 3,
    "file_permissions": {
      "read": [],
      "write": []
    },
    "network_whitelist": []
  },
  "skill_id": "uuid_generator",
  "input_schema": {
    "required_params": [
      {
        "name": "count",
        "type": "number",
        "description": "Number of UUIDs to generate"
      }
    ],
    "optional_params": []
  },
  "output_schema": {
    "fields": [
      {
        "name": "uuids",
        "type": "array",
        "description": "Generated UUIDs"
      }
    ]
  },
  "execution_dag": {
    "nodes": [
      {
        "id": "main",
        "type": "transform",
        "action": "uuid_generator",
        "params": {}
      }
    ],
    "edges": [],
    "entry_node": "main",
    "exit_node": "main"
  },
  "mcp_compatible": false
}