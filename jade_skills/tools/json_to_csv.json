{
  "jade_version": "1.0.0",
  "skill_id": "json_to_csv",
  "metadata": {
    "name": "JSON to CSV Converter",
    "version": "1.0.0",
    "description": "Convert JSON array to CSV format.",
    "author": "jadegate",
    "tags": [
      "json",
      "csv",
      "convert",
      "data"
    ],
    "license": "BSL-1.1",
    "created_at": "2026-02-22T17:50:21Z",
    "updated_at": "2026-02-22T17:50:21Z"
  },
  "trigger": {
    "type": "task_intent",
    "conditions": [
      {
        "field": "task.type",
        "operator": "in",
        "value": [
          "json_to_csv"
        ]
      }
    ]
  },
  "input_schema": {
    "required_params": [
      {
        "name": "json_data",
        "type": "string",
        "description": "JSON array string to convert"
      }
    ],
    "optional_params": []
  },
  "output_schema": {
    "fields": [
      {
        "name": "csv_content",
        "type": "string",
        "description": "CSV output"
      },
      {
        "name": "status",
        "type": "string",
        "description": "Status"
      }
    ]
  },
  "execution_dag": {
    "nodes": [
      {
        "id": "parse",
        "action": "json_parse",
        "params": {
          "data": "{{input.json_data}}"
        }
      },
      {
        "id": "convert",
        "action": "data_transform",
        "params": {
          "input": "{{parse.output.value}}",
          "format": "csv"
        }
      },
      {
        "id": "ret",
        "action": "return_result",
        "params": {
          "csv_content": "{{convert.output.value}}",
          "status": "ok"
        }
      }
    ],
    "edges": [
      {
        "from": "parse",
        "to": "convert"
      },
      {
        "from": "convert",
        "to": "ret"
      }
    ],
    "entry_node": "parse",
    "exit_node": [
      "ret"
    ]
  },
  "security": {
    "network_whitelist": [],
    "file_permissions": {
      "read": [],
      "write": []
    },
    "max_execution_time_ms": 10000,
    "max_retries": 1,
    "sandbox_level": "strict",
    "dangerous_patterns": []
  },
