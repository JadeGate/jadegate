{
  "jade_version": "1.0.0",
  "skill_id": "xml_to_json",
  "metadata": {
    "name": "XML to JSON Converter",
    "version": "1.0.0",
    "description": "Parse XML input and convert to JSON structure.",
    "author": "jadegate",
    "tags": [
      "xml",
      "json",
      "convert",
      "parse"
    ],
    "license": "BSL-1.1",
    "created_at": "2026-02-22T17:50:21Z",
    "updated_at": "2026-02-22T17:50:21Z"
  },
  "trigger": {
    "type": "task_intent",
    "conditions": [
      {
        "field": "task.type",
        "operator": "in",
        "value": [
          "xml_to_json"
        ]
      }
    ]
  },
  "input_schema": {
    "required_params": [
      {
        "name": "xml_content",
        "type": "string",
        "description": "XML string to convert"
      }
    ],
    "optional_params": []
  },
  "output_schema": {
    "fields": [
      {
        "name": "json_data",
        "type": "object",
        "description": "Converted JSON"
      },
      {
        "name": "status",
        "type": "string",
        "description": "Status"
      }
    ]
  },
  "execution_dag": {
    "nodes": [
      {
        "id": "parse",
        "action": "xml_parse",
        "params": {
          "data": "{{input.xml_content}}"
        }
      },
      {
        "id": "ret",
        "action": "return_result",
        "params": {
          "json_data": "{{parse.output.value}}",
          "status": "ok"
        }
      }
    ],
    "edges": [
      {
        "from": "parse",
        "to": "ret"
      }
    ],
    "entry_node": "parse",
    "exit_node": [
      "ret"
    ]
  },
