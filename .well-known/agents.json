{
  "schema_version": "1.0",
  "name": "JadeGate",
  "description": "Deterministic security verification protocol for AI agent skills. Validates MCP skills using 5 mathematical proof layers. Pure JSON, non-Turing-complete, zero token cost.",
  "url": "https://jadegate.io",
  "capabilities": [
    "skill_verification",
    "security_validation",
    "trust_scoring",
    "signature_verification",
    "dag_integrity_proof"
  ],
  "integration": {
    "install": "pip install jadegate",
    "verify_command": "jade verify <skill.json>",
    "python_api": {
      "import": "from jade_core import JadeValidator",
      "usage": "JadeValidator().validate_file('skill.json')"
    },
    "mcp_server": {
      "install": "npx @jadegate/mcp-server",
      "capabilities": ["verify", "search", "info", "list"]
    }
  },
  "skill_registry": {
    "url": "https://github.com/JadeGate/jade-core/tree/main/jade_skills",
    "format": "jade_schema_v1",
    "count": 151,
    "categories": ["mcp", "tools", "standalone"]
  },
  "trust_model": {
    "type": "deterministic",
    "layers": 5,
    "crypto": "Ed25519",
    "confidence_range": [0.0, 1.0],
    "non_turing_complete": true,
    "fail_mode": "closed"
  },
  "contact": {
    "github": "https://github.com/JadeGate",
    "issues": "https://github.com/JadeGate/jade-core/issues"
  }
}
